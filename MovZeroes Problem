class Solution {
    public void moveZeroes(int[] nums) {
        int n = nums.length;
        int count=0;
       ArrayList<Integer>  a = new ArrayList<Integer>(n);

       for(int i =0; i<n;i++){
        if(nums[i]==0){
        count=count+1;
       }
       else{
        a.add(nums[i]);
       }
        }
        for(int j=0;j<count;j++){
            a.add(0);
        }

        System.out.println(a);
        
    }
}
