class Solution {
    public boolean containsDuplicate(int[] nums) {
       int n= nums.length;
       HashMap<Integer,Integer> hm = new HashMap<Integer,Integer>();

       for(int i=0;i<n;i++){
        hm.put(nums[i],i);
       }
       for(int i=0;i<n;i++){
       
        if(hm.containsKey(nums[i]) && hm.get(nums[i])!=i){
            return true;
        }
       }
     return false;
        
    }
}
